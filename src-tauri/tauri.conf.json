{
  "productName": "gipidi",
  "version": "0.1.0",
  "identifier": "com.jujiplay.gipidi",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "bun run dev:next",
    "beforeBuildCommand": "bun run build:next"
  },
  "app": {
    "windows": [
      {
        "title": "GiPiDi'",
        "width": 950,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 300,
        "minHeight": 500,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
